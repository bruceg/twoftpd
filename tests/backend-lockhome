<requires home>
LOCKHOME=1
export LOCKHOME
xfertest xfer port retr asc PWD NLST
echo
xfertest xfer port retr asc 'CWD subdir' PWD NLST
echo
xfertest xfer port retr asc 'CWD ..' PWD NLST
unset LOCKHOME
<result>
