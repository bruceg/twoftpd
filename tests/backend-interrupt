<requires home>
<loop mode pasv port>
for i in $( seq 10000 ); do
  echo 012345678901234567890123456789012345678901234567890123456789012
done >$home/testfile
xfertest xfer $mode retr bin 64 'RETR testfile' | fixbytes
rm $home/testfile
