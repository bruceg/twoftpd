<asserts good-cvm>
makecvm 'cat >/dev/null' \
	'printf "\\0"' \
	'printf "\\001username\\0"' \
	'printf "\\002102\\0"' \
	'printf "\\003103\\0"' \
	'printf "\\004Real Name\\0"' \
	'printf "\\005/home/dir\\0"' \
	'printf "\\006/bin/false\\0"' \
	'printf "\\007grpname\\0"' \
	'printf "\\000"' \
	'exit 0'
<result>
